COLUMN TABLE "BASE_LOCAL.MLSCENARIO_MODELREPOSITORY" (
     "MODEL_NAME" NVARCHAR(100),
	 "MODEL_VERSION" DOUBLE CS_DOUBLE,
	 "MODEL_TYPE" NVARCHAR(100),
	 "ROW_INDEX" INTEGER CS_INT,
	 "TREE_INDEX" INTEGER CS_INT,
	 "MODEL_CONTENT" NVARCHAR(5000),
	 "KEY" NVARCHAR(1000),
	 "VALUE" NVARCHAR(1000),
	 "MODEL_DATE" DATE CS_DAYDATE DEFAULT CURRENT_DATE,
	 "MODEL_TIME" TIME CS_SECONDTIME DEFAULT CURRENT_TIME) UNLOAD PRIORITY 5 AUTO MERGE 